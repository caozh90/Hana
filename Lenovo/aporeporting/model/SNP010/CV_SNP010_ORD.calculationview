                  demand_week_19 != 0 or
                  demand_week_20 != 0 or
                  demand_week_21 != 0 or
                  demand_week_22 != 0 or
                  demand_week_23 != 0 or
                  demand_week_24 != 0 or
                  demand_week_25 != 0 or
                  demand_week_26 != 0 or
                  demand_week_27 != 0 or      
