end as demand_week_46,
case
when B.id =47
then A.real_quantity
else 0
end as demand_week_47,
case
when B.id =48
then A.real_quantity
else 0
end as demand_week_48,
case
when B.id =49
then A.real_quantity
else 0
end as demand_week_49,
case
when B.id =50
then A.real_quantity
else 0
end as demand_week_50,
case
when B.id =51
then A.real_quantity
else 0
end as demand_week_51
 	   from :s_filt as A inner join :s_cal as B on A.week_no = B.week_no;
 
 var_out = select mandt, orderid ,matid, locid, versiod, fcst_matnr, geo_level, --zcto,
                  real_quantity,
                  demand_week_0,
                  demand_week_1,
                  demand_week_2,
                  demand_week_3,
                  demand_week_4,
                  demand_week_5,
                  demand_week_6,
