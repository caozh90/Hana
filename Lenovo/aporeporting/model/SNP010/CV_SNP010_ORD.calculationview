 /********* Begin Procedure Script ************/ 
 BEGIN 
       
       declare idx Integer :=1;
       DECLARE id INTEGER ARRAY; 
       DECLARE week_no NVARCHAR(10) ARRAY;
       
       While idx&lt;=52 do
       	id[idx] := idx-1; 
       	week_no[idx] := isoweek(ADD_DAYS(current_date, (idx-1)*7)); 
       	idx := :idx+1;
       end while;

       s_cal = UNNEST(:id, :week_no) AS (&quot;ID&quot;, &quot;WEEK_NO&quot;);            
 	           
 	   s_filt = select A.mandt, A.ORDERID, A.MATID, A.LOCID,A.VERSIOD, A.FCST_MATNR, A.geo_level, A.local_date, A.category, A.category_type,-- A.zcto,
 	   isoweek(case when A.local_date &lt;current_date then current_date else A.local_date end) 	   
 	   as week_no, 
 	   case 
 	       when A.category_type= 2 
 	           then abs(A.real_quantity)
 	       when A.category_type= 3
 	           then abs(A.real_quantity)
 	       else A.real_quantity 
 	           end as real_quantity
 	   from &quot;ZTSNP_REP_ORD&quot; as A inner join &quot;_SYS_BIC&quot;.&quot;aporeporting.model.SNP010/CV_SNP010_ATP&quot; as B
 	   on A.mandt = B.mandt and A.category = B.atpcat;

       t_not_filter = select A.mandt,A.ORDERID, A.MATID, A.LOCID, A.VERSIOD, A.FCST_MATNR, A.geo_level,  A.real_quantity, --A.zcto, 
 	   case
when B.id =0
then A.real_quantity
else 0
end as demand_week_0,
case
when B.id =1
then A.real_quantity
else 0
end as demand_week_1,
case
when B.id =2
then A.real_quantity
else 0
end as demand_week_2,
case
when B.id =3
then A.real_quantity
else 0
end as demand_week_3,
case
when B.id =4
then A.real_quantity
else 0
end as demand_week_4,
case
when B.id =5
then A.real_quantity
else 0
end as demand_week_5,
case
when B.id =6
then A.real_quantity
else 0
end as demand_week_6,
case
when B.id =7
then A.real_quantity
else 0
end as demand_week_7,
case
when B.id =8
then A.real_quantity
else 0
end as demand_week_8,
case
when B.id =9
then A.real_quantity
else 0
end as demand_week_9,
case
when B.id =10
then A.real_quantity
else 0
end as demand_week_10,
