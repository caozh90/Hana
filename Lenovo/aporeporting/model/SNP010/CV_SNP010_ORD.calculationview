end as demand_week_25,
case
when B.id =26
then A.real_quantity
else 0
end as demand_week_26,
case
when B.id =27
then A.real_quantity
else 0
end as demand_week_27,
case
when B.id =28
then A.real_quantity
else 0
end as demand_week_28,
case
when B.id =29
then A.real_quantity
else 0
end as demand_week_29,
case
when B.id =30
then A.real_quantity
else 0
end as demand_week_30,
case
when B.id =31
then A.real_quantity
else 0
end as demand_week_31,
case
when B.id =32
then A.real_quantity
else 0
end as demand_week_32,
case
when B.id =33
then A.real_quantity
else 0
end as demand_week_33,
case
when B.id =34
then A.real_quantity
else 0
end as demand_week_34,
