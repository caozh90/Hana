<mvc:View controllerName="lenovo.mtm.controller.Object" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m">
	<Page id="Objectpage" 
	      navButtonPress="onNavBack_o" 
	      showNavButton="true"
		  title="{i18n>Object}"
		  class="MTM-title">
	  <content>
	    <Table id="ObjectTable" items="{MTMLIST}">
          <columns>
            <Column id="Column1" width="6em" >

		    </Column>
		    <Column id="Column2" width="6em" minScreenWidth="Tablet" demandPopin="true">

		    </Column>
		  </columns>
	      <items>
	      	<ColumnListItem>
	      		<cells>
	      			<ObjectIdentifier
	      				title="{Zdesc}" />
	      			<Text
						text="{Zvalue}" />
	      		</cells>
	      	</ColumnListItem>
	      </items>
	    </Table>
	  </content>
	</Page>
</mvc:View>