<mvc:View
	controllerName="demo.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<App id="Appaa">
		<pages>
			<Page
				id="pageaaa"
				title="OverPageTitle"
				height="100%"
				showNavButton="true"
				navButtonText="close"
				navButtonPress="pressNavButton"
				>
				<headerContent>
					<Button
						icon="sap-icon://hello-world"
						press="onOpenDialog"
					/>
				</headerContent>
				<content>
					<mvc:XMLView viewName="demo.view.Grid" id="splitContainer"/>
					<!--  <mvc:XMLView viewName="demo.view.InvoiceList" /> -->
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>